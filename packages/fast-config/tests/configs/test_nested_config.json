{
  "api": {
    "endpoints": {
      "users": "/api/v1/users",
      "auth": "/api/v1/auth"
    },
    "rate_limits": {
      "per_minute": 100,
      "burst": 200
    }
  },
  "cache": {
    "redis": {
      "host": "localhost",
      "port": 6379,
      "db": 0
    },
    "ttl": 3600
  },
  "monitoring": {
    "enabled": true,
    "metrics": {
      "prometheus": {
        "enabled": true,
        "port": 9090
      }
    }
  }
}